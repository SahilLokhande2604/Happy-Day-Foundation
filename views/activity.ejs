<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Activity</title>
  <link rel="stylesheet" href="/css/style5_part1.css">
  <link rel="stylesheet" href="css/header.css">
  <link rel="stylesheet" href="css/footer.css">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
  </head>

<body>
  <%- include("partials/header.ejs", { headerData: data.headerData,socialMediaData: data.socialMediaData }) %>
  
  <div class="container">
  <div class="top">

    <img class="cover" src="/images/<%=data.activityData.top_image %>" class="img-fluid" alt="...">
    <div class="title animation-target"><%= data.top_title %></div>
  </div>
  <div class="content">
    <% for (const contentElement of data.activityData.content) { %>
      <div class="card box">
          <a class="activity_link" href="/public/<%= contentElement.content_element_name %>">
              <img src="/images/<%=contentElement.content_images%>"
                  class="card-img-top after_hover img-cover" alt="...">
              <div class="card-body box_body animation-target">
                  <h5 class="card-title"> <%= contentElement.content_title %></h5>
                  <p class="card-text"> <%= contentElement.content_description %></p>
                  <a class="a_on_hover" href="/public/<%= contentElement.content_element_name %>">Read more...</a>
              </div>
          </a>
      </div>
  <% } %>
    
  </div>
  </div>

  <%- include("partials/footer.ejs",{footerData: data.footerData, socialMediaData: data.socialMediaData ,contactUsData: data.contactUsData}) %>


 
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.4/dist/jquery.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js"></script>
    <script src="/css/sript4.js"></script>
   
</body>

</html>